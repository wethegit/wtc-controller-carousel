{
  "name": "wtc-controller-carousel",
  "version": "1.0.0",
  "main": "dist/wtc-controller-carousel.js",
  "style": "dist/wtc-controller-carousel.css",
  "scripts": {
    "prebuild-js": "babel src/ --presets @babel/preset-env --out-dir build",
    "prebuild-css": "node-sass --output-style compressed src/wtc-controller-carousel.scss build/wtc-controller-carousel.css",
    "minify-js": "uglifyjs build/wtc-controller-carousel.js --compress --output dist/wtc-controller-carousel.min.js",
    "minify-css": "postcss build/wtc-controller-carousel.css --no-map --use autoprefixer -d dist/",
    "build": "npm run prebuild-js && npm rum prebuild-css",
    "prepare": "npm run build && webpack --config webpack.config.js && npm run minify-js && npm run minify-css"
  },
  "repository": "https://github.com/wethegit/wtc-controller-carousel",
  "author": "Marlon Marcello <marlon.marcello@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.3",
    "uglify-js": "^3.6.0",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "3",
    "wtc-controller-element": "^1.1.1",
    "wtc-utility-breakpoint": "^1.0.6"
  }
}
